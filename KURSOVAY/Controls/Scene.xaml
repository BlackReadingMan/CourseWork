<UserControl x:Class="KURSOVAY.Controls.Scene"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KURSOVAY.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <Grid>
        <Image x:Name="Picture" Panel.ZIndex="0" ></Image>
        <Label x:Name="RenderTimer" FontSize="20" Panel.ZIndex="1" HorizontalAlignment="Right" VerticalAlignment="Top" Foreground="White"/>
        <Button x:Name="BulletCatcher" HorizontalAlignment="Left" Margin="1,0,0,0" VerticalAlignment="Top" KeyDown="Win_KeyDown" Background="#00DDDDDD" BorderBrush="#00707070" Foreground="#00000000"/>
    </Grid>
</UserControl>
